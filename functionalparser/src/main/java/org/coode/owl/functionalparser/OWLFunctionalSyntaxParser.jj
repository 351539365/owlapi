public IRI IRI() :
{
    IRI iri;
}
{
    (iri=FullIRI() | iri=AbbreviatedIRI()) {
        return iri;
    }
}

public IRI FullIRI() :
{
    Token t;
}
{
    t=<FULLIRI> {
        try {
            return dataFactory.getIRI(getURI(t.image));
        }
        catch(URISyntaxException e) {
            throw new OWLRuntimeException(e);
        }
    }
}

public IRI AbbreviatedIRI() :
{
    Token t;
}
{
    t=<PNAME_LN> {
        try {
            return dataFactory.getIRI(getURI(t.image));
        }
        catch(URISyntaxException e) {
            throw new OWLRuntimeException(e);
        }
    }
}

public String PrefixName() :
{
    Token t;
}
{
    t=<PNAME_NS> {
        return t.image;
    }
}



